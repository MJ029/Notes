    "import pandas as pd\n",
    "def make_df(cols,ind):\n",
    "    data={c:[str(c)+str(i) for i in ind]\n",
    "         for c in cols}\n",
    "    print(data)\n",
    "    return pd.DataFrame(data,ind)\n",
    "make_df('HIJK',range(3))"
   ]
